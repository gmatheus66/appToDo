<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".Register" android:background="@color/back">
    <androidx.constraintlayout.widget.Guideline
            android:orientation="vertical"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" app:layout_constraintGuide_begin="27dp" android:id="@+id/guideline19"/>
    <androidx.constraintlayout.widget.Guideline
            android:orientation="vertical"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" app:layout_constraintGuide_begin="377dp"
            android:id="@+id/guideline20"/>
    <EditText
            android:layout_width="235dp"
            android:layout_height="44dp"
            android:inputType="textPersonName"
            android:text="Name"
            android:ems="10"
            android:id="@+id/editTextName"
            android:textColor="@color/white" android:textColorHint="@color/white"
            android:background="@color/colorPrimary" android:layout_marginTop="116dp"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginStart="8dp"
            app:layout_constraintStart_toStartOf="@+id/guideline19" android:layout_marginLeft="8dp"
            android:layout_marginEnd="8dp" app:layout_constraintEnd_toStartOf="@+id/guideline20"
            android:layout_marginRight="8dp" app:layout_constraintHorizontal_bias="0.515"/>
    <EditText
            android:layout_width="235dp"
            android:layout_height="44dp"
            android:inputType="textEmailAddress"
            android:ems="10"
            android:id="@+id/editTextEmail"
            android:background="@color/colorPrimary" android:layout_marginTop="48dp"
            app:layout_constraintTop_toBottomOf="@+id/editTextName" android:layout_marginStart="56dp"
            app:layout_constraintStart_toStartOf="@+id/guideline19" android:layout_marginLeft="56dp"
            android:layout_marginEnd="8dp" app:layout_constraintEnd_toStartOf="@+id/guideline20"
            android:layout_marginRight="8dp" app:layout_constraintHorizontal_bias="0.078"
            android:textColor="@color/white" android:textColorHint="@color/white"
            android:hint="Email"/>
    <EditText
            android:layout_width="235dp"
            android:layout_height="44dp"
            android:inputType="numberPassword"
            android:ems="10"
            android:id="@+id/editText3" android:layout_marginTop="40dp"
            app:layout_constraintTop_toBottomOf="@+id/editTextEmail" android:layout_marginStart="8dp"
            app:layout_constraintStart_toStartOf="@+id/guideline19" android:layout_marginLeft="8dp"
            android:layout_marginEnd="8dp" app:layout_constraintEnd_toStartOf="@+id/guideline20"
            android:layout_marginRight="8dp" android:hint="Password" android:textColorHint="@color/white"
            android:textColor="@color/white" android:background="@color/colorPrimary"
            app:layout_constraintHorizontal_bias="0.519"/>
    <Button
            android:text="Register"
            android:layout_width="167dp"
            android:layout_height="59dp"
            android:id="@+id/btnRes" app:layout_constraintEnd_toStartOf="@+id/guideline20"
            android:layout_marginEnd="8dp" android:layout_marginRight="8dp"
            app:layout_constraintStart_toStartOf="@+id/guideline19" android:layout_marginLeft="8dp"
            android:layout_marginStart="8dp" android:layout_marginBottom="104dp"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintHorizontal_bias="0.52"
            android:background="@color/green" android:textColor="@color/white" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/editText3" app:layout_constraintVertical_bias="0.854"/>
    <androidx.constraintlayout.widget.Guideline
            android:orientation="vertical"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" app:layout_constraintGuide_begin="87dp" android:id="@+id/guideline21"/>
</androidx.constraintlayout.widget.ConstraintLayout>